# 环境配置模板 - Railway + Vercel部署
# 请复制此文件为.env并填入您的实际参数

# 环境配置
NODE_ENV=production
PORT=3000

# 数据库配置 (Railway MySQL服务自动提供以下变量)
# DB_HOST=${{ MYSQLHOST }}
# DB_PORT=${{ MYSQLPORT }}
# DB_NAME=${{ MYSQLDATABASE }}
# DB_USER=${{ MYSQLUSER }}
# DB_PASSWORD=${{ MYSQLPASSWORD }}

# Redis配置 (Railway Redis服务自动提供以下变量)
# REDIS_HOST=${{ REDISHOST }}
# REDIS_PORT=${{ REDISPORT }}
# REDIS_PASSWORD=${{ REDISPASSWORD }}

# JWT配置 (必须手动设置)
JWT_SECRET=your_super_secret_jwt_key_here_make_it_long_and_random_at_least_32_characters
JWT_EXPIRES_IN=7d

# AWS S3配置 (必须手动设置)
AWS_ACCESS_KEY_ID=您的AWS_AccessKey_ID
AWS_SECRET_ACCESS_KEY=您的AWS_SecretAccessKey
AWS_S3_BUCKET=您的S3_bucket名称
AWS_S3_REGION=您的S3_region(如:us-east-1)
AWS_S3_UPLOAD_ENDPOINT=https://s3.amazonaws.com
AWS_S3_ACCESS_DOMAIN=https://jbimage.image.ccoin.click

# CORS配置 (必须设置为Vercel前端地址)
FRONTEND_URL=https://your-app.vercel.app

# 管理员初始账号配置
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123
ADMIN_EMAIL=admin@example.com

# 文件上传配置
MAX_FILE_SIZE=5242880
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp

# 日志配置
LOG_LEVEL=info
LOG_DIR=logs